{
    "name": "gildastema/eureka-sms-notification",
    "description": "Notification message for Eureka SMS",
    "keywords": [
        "gildastema",
        "eureka-sms-notification"
    ],
    "homepage": "https://github.com/gildastema/eureka-sms-notification",
    "license": "MIT",
    "type": "library",
    "authors": [
        {
            "name": "Gildas Tema",
            "email": "gildastema3@gmail.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^7.1",
        "guzzlehttp/guzzle": "^7.1",
        "illuminate/support": "^6.0|^7.0|^8.0"
    },
    "require-dev": {
        "orchestra/testbench": "^4.0|^5.0|^6.0",
        "phpunit/phpunit": "^8.0|^9.0"
    },
    "autoload": {
        "psr-4": {
            "Eureka\\SMS\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Eureka\\SMS\\Tests\\": "tests"
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit",
        "test-coverage": "vendor/bin/phpunit --coverage-html coverage"

    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "providers": [
                "Gildastema\\EurekaSmsNotification\\EurekaSmsNotificationServiceProvider"
            ],
            "aliases": {
                "EurekaSmsNotification": "Gildastema\\EurekaSmsNotification\\EurekaSmsNotificationFacade"
            }
        }
    }
}
